{
	"name": "new-vxp-workspace",
	"private": true,
	"version": "0.0.1",
	"description": "",
	"scripts": {
		"format": "pnpm exec biome format --write",
		"lint": "pnpm exec biome lint --write",
		"dev:all": "concurrently \"pnpm --filter api --parallel run start:dev\" \"pnpm --filter web run dev\" \"pnpm --filter docs run dev\" \"pnpm --filter app run dev\"",
		"nest:api": "pnpm --filter api exec -- nest",
		"dev:api": "pnpm --filter api run start:dev",
		"build:api": "pnpm --filter api run build",
		"lint:api": "pnpm --filter api exec biome lint --write",
		"format:api": "pnpm --filter api exec biome format --write",
		"prod:api": "pnpm --filter api run start:prod",
		"test:api": "pnpm --filter api run test",
		"test:watch:api": "pnpm --filter api run test:watch",
		"test:cov:api": "pnpm --filter api run test:cov",
		"dev:web": "pnpm --filter web run dev",
		"build:web": "pnpm --filter web run build",
		"preview:web": "pnpm --filter web run preview",
		"format:web": "pnpm --filter web exec biome format --write",
		"lint:web": "pnpm --filter web exec biome lint --write",
		"dev:docs": "pnpm --filter docs run dev",
		"start:docs": "pnpm --filter docs run start",
		"build:docs": "pnpm --filter docs run build",
		"preview:docs": "pnpm --filter docs run preview",
		"format:docs": "pnpm --filter docs exec biome format --write",
		"lint:docs": "pnpm --filter docs exec biome lint --write",
		"dev:app": "pnpm --filter app run dev",
		"build:app": "pnpm --filter app run build",
		"preview:app": "pnpm --filter app run preview",
		"format:app": "pnpm --filter app exec biome format --write",
		"lint:app": "pnpm --filter app exec biome lint --write"
	},
	"keywords": [],
	"author": "Arpit Verma",
	"license": "ISC",
	"packageManager": "pnpm@10.13.1",
	"devDependencies": {
		"@biomejs/biome": "2.2.3",
		"@nestjs/cli": "^11.0.10",
		"concurrently": "^9.2.1"
	}
}
